{
  "name": "Handsfree Chrome",
  "version": "0.0.1",
  "description": "Browse the web, watch videos, play games, shop, use social media, and more all handsfree by using face gestures to control a pointer!",
  "manifest_version": 2,

  "icons": {
    "16": "public/img/favicon.png",
    "32": "public/img/favicon.png",
    "48": "public/img/favicon.png",
    "64": "public/img/favicon.png"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "public/img/favicon.png",
      "32": "public/img/favicon.png",
      "48": "public/img/favicon.png",
      "64": "public/img/favicon.png"
    }
  }
}
